{
  "manifest_version": 3,
  "name": "Gems for AI Studio",
  "version": "1.0.0",
  "description": "Save and load reusable System Instructions in Google AI Studio.",
  "permissions": [
    "storage"
  ],
  "host_permissions": [
    "*://aistudio.google.com/*"
  ],
  "content_scripts": [
    {
      "matches": ["*://aistudio.google.com/*"],
      "js": ["content.js"],
      "css": ["style.css"]
    }
  ],
  "icons": {
    "16": "icon.png",
    "48": "icon.png",
    "128": "icon.png"
  }
}
